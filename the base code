from statistics import *
lst = [0, 0, 0, 1, 3, 3, 3, 4, 5, 5, 5, 7, 7, 7, 10, 10, 12, 15, 15, 18]

number = len(lst)
print("number = %d"%number)

med = median(lst)
print("median = %d"%med)

mean = mean(lst)
print("mean = %d"%mean)


#   First sort the list
print(lst)
lst.sort()
print(lst)
#   Then get the element a quarter of the way through it")

#   for instance, len(lst) / 4. Except that value is a Float, and you need an integer.

print(len(lst)/4)

